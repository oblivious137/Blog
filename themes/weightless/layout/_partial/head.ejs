<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">

	<%
		var title = page.title;
	%>

	<title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>
	<meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">
	<% if (config.lunr) { %>
		<script src="<%= url_for('js/compatibility/augment.js') %>" type="text/javascript"></script>
	<% } %>
	<% if (config.feed) { %>
		<% if (config.feed.type == 'atom') { %>
			<link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>" type="application/atom+xml" />
		<% }else if (config.feed.type == 'rss2') { %>
			<link rel="alternate" href="<%= url_for(config.feed.path) %>" title="<%= config.title %>" type="application/rss+xml" />
		<% } %>
	<% } %>
	<link rel="stylesheet" href="<%= url_for('css/normalize.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/basscss.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/basscss-ui-groups.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/tomorrow.css') %>" type="text/css">
	<link rel="stylesheet" href="<%= url_for('css/style.css') %>" type="text/css">
	<% if (theme.useGoogle) { %>
		<link href="http://fonts.googleapis.com/css?family=Lato:300,400,700,400italic&subset=latin,latin-ext" rel="stylesheet" type="text/css">
	<% }else{ %>
		<link href="https://dist.s3.fmt01.sdapi.net/fonts/lato-v11/lato-v11-all.css" rel="stylesheet" type="text/css">
	<% } %>
	<% if (config.google_site_verification) { %>
		<meta name="google-site-verification" content="<%= config.google_site_verification %>" />
	<% } %>

	<!--[if lt IE 9]><script src="<%- config.root %>js/html5.js" type="text/javascript"></script><![endif]-->
	<script src="<%= url_for('js/main.js') %>" type="text/javascript"></script>
</head>
